{
  "name": "svelte-chainstore",
  "version": "0.1.2",
  "description": "Perform a chain of operations in response to store changes.",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "test": "jest src/tests --config jest.config.json --env=jsdom"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.16.0",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@types/jest": "^27.0.3",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "babel-jest": "^27.4.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "jest": "^27.4.3",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.6",
    "svelte-jester": "^2.1.5",
    "svelte-preprocess": "^4.9.4",
    "svelte2tsx": "^0.4.10",
    "ts-jest": "^27.1.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/louislouw/svelte-chainstore.git"
  },
  "keywords": [
    "svelte",
    "chainstore",
    "chainlink",
    "dirtystore",
    "autosave"
  ],
  "author": "Louis Louw",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/louislouw/svelte-chainstore/issues"
  },
  "homepage": "https://github.com/louislouw/svelte-chainstore#readme"
}